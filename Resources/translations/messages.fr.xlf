            <trans-unit id="300">
                <source>commandeScheduler.flash.success</source>
                <target>Planification sauvegardée avec succés.</target>
            </trans-unit>
            <trans-unit id="301">
                <source>commandeScheduler.flash.deleted</source>
                <target>Planification supprimée avec succés.</target>
            </trans-unit>
            <trans-unit id="302">
                <source>commandeScheduler.flash.execute</source>
                <target>Le traitement sera exécutée lors de la prochaine vérification (scheduler:execute).</target>
            </trans-unit>
            <trans-unit id="303">
                <source>commandeScheduler.flash.unlocked</source>
                <target>Traitement dévérouillée avec succès.</target>
            </trans-unit>

