            </trans-unit>
            <trans-unit id="3">
                <source>commandeScheduler.list.title</source>
                <target>Taches planifiées</target>
            </trans-unit>

            <!-- Actions -->
            <trans-unit id="100">
                <source>commandeScheduler.action.delete</source>
                <target>Supprimer</target>
            </trans-unit>
            <trans-unit id="101">
                <source>commandeScheduler.action.execute</source>
